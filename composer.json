{
    "name": "invis1ble/messenger-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for the Bus and Message Interfaces and Implementations.",
    "keywords": [
        "event",
        "command",
        "query",
        "messenger",
        "message-bus",
        "event-bus",
        "command-bus",
        "query-bus",
        "cqrs",
        "symfony-bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Invis1ble",
            "email": "Invis1ble@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "invis1ble/messenger": "^2.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Invis1ble\\MessengerBundle\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^10.4@dev"
    }
}
