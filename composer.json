{
    "name": "invis1ble/messenger-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for the Bus and Message Interfaces and Implementations.",
    "authors": [
        {
            "name": "Invis1ble",
            "email": "Invis1ble@users.noreply.github.com"
        }
    ],
    "keywords": [
        "event",
        "command",
        "query",
        "messenger",
        "message-bus",
        "event-bus",
        "command-bus",
        "query-bus",
        "cqrs",
        "symfony-bundle"
    ],
    "require": {
        "php": ">=8.2",
        "invis1ble/messenger": "^3.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.0@dev",
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpunit/phpunit": "^10",
        "matthiasnoback/symfony-dependency-injection-test": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Invis1ble\\MessengerBundle\\": "src/",
            "Invis1ble\\MessengerBundle\\Tests\\": "tests/"
        },
        "exclude-from-classmap": ["/tests/"]
    },
    "minimum-stability": "dev",
    "license": "MIT"
}
